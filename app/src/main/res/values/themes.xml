<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.Timeline_travel" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:windowBackground">@color/white</item>"
    </style>

    <style name="Theme.MySplashScreen" parent="Theme.Timeline_travel">
        <!-- 배경 색상 설정: 흰색 -->
        <item name="android:windowSplashScreenBackground" tools:ignore="NewApi">@android:color/white</item>

        <!-- 로고 설정: 투명 -->
        <item name="android:windowSplashScreenAnimatedIcon" tools:ignore="NewApi">@android:color/transparent</item>
    </style>
</resources>